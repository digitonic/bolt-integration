{
    "name": "digitonic/bolt",
    "description": "Bolt integration",
    "type": "laravel-plugin",
    "authors": [
        {
            "name": "Rodolfo Perez",
            "email": "rodolfo@digitonic.co.uk"
        },
        {
            "name": "Yannick Glady",
            "email": "yannick@digitonic.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Digitonic\\Bolt\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Digitonic\\Bolt\\BoltServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "5.8.*|6.*",
        "guzzlehttp/guzzle": "6.5.*",
        "illuminate/database": "5.8.*|6.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4"
    },
    "minimum-stability": "dev"
}
